t.basics = Grundlegende Kontrollen
m.intro.basics = Beweg' dich!
m.instructions.basics = <b>W</b>: Vorwärts bewegen\n\
<b>A</b>: Nach links bewegen\n\
<b>S</b>: Rückwärts bewegen \n\
<b>D</b>: Nach rechts bewegen\n\
\n\
<b>Shift</b>: Sprinten (verbraucht Energie) \n\
\n\
<b>Leertaste</b>: Springen (halten, um Jetpack zu verwenden, wenn vorhanden) \n\
\n\
<b>E</b>: Interaktion (wenn angeboten)
m.instructions.gamepad.basics = <b>Linker Stick</b>: Bewegen\n\
\n\
<b>Linken Stick halten</b>: Sprinten (verbraucht Energie) \n\
\n\
<b>A</b>: Springen (halten, um Jetpack zu verwenden, wenn vorhanden) \n\
\n\
<b>X</b>: Interaktion (wenn angeboten)
m.instructions.ps4.basics = <b>linker Stick</b>: Bewegen\n\
\n\
<b>L3</b>: Sprinten (verbraucht Energie) \n\
\n\
<b>Kreuz-Taste</b>: Springen (halten, um Jetpack zu verwenden, wenn vorhanden) \n\
\n\
<b>Quadrat-Taste</b>: Interaktion (wenn angeboten)
